{
  "main": "app.ts",
  "private": true,
  "license": "ISC",
  "name": "dashboard",
  "version": "1.0.0",
  "author": "Vitor Barcelos",
  "description": "DAPI: Dashboard API",
  "nodemonConfig": {
    "ext": "ts, js, json, graphql",
    "exec": "yarn run dev",
    "watch": [
      "./app/",
      "./tests/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "watch:debug": "nodemon --inspect",
    "watch:test": "jest --detectOpenHandles --forceExit --config jest.json",
    "watch:dev": "nodemon",
    "dev": "yarn run server NODE_ENV=development",
    "prod": " yarn run server NODE_ENV=production",
    "server": "node -r ts-node/register ./app/www",
    "knex": "knex --knexfile knexfile.ts",
    "seed:do": "yarn run knex seed:run",
    "migrate:do": "yarn run knex migrate:latest",
    "migrate:undo": "yarn run knex migrate:rollback --all",
    "prettier:check": "prettier --check ./",
    "prettier:write": "prettier --write ./",
    "lint": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/lodash": "^4.14.159",
    "@types/lodash-es": "^4.17.3",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.6.0",
    "@types/socket.io": "^2.1.11",
    "cookie-parser": "~1.4.4",
    "express": "^5.0.0-alpha.8",
    "express-graphql": "^0.11.0",
    "graphql": "^15.3.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-tools": "^6.1.0",
    "knex": "^0.21.5-next2",
    "lodash": "^4.17.20",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.4",
    "objection": "^2.2.2",
    "pg": "^8.3.2",
    "socket.io": "^2.3.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0"
  }
}
